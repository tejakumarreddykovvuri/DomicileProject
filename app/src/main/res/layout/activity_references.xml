<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".references">

    <TextView
        android:id="@+id/Locations_HIC"
        android:layout_width="408dp"
        android:layout_height="38dp"
        android:ems="10"
        android:hint="@string/HIC"
        android:importantForAutofill="no"
        android:text="@string/HIC"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Locations_AP" />

    <TextView
        android:id="@+id/Locations_CIC"
        android:layout_width="408dp"
        android:layout_height="38dp"
        android:ems="10"
        android:hint="@string/CIC"
        android:importantForAutofill="no"
        android:text="@string/CIC"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.333"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Locations_LL" />

    <TextView
        android:id="@+id/Locations_LL"
        android:layout_width="408dp"
        android:layout_height="38dp"
        android:ems="10"
        android:hint="@string/LL"
        android:importantForAutofill="no"
        android:text="@string/LL"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Locations_TC" />

    <TextView
        android:id="@+id/Locations_AP"
        android:layout_width="408dp"
        android:layout_height="38dp"
        android:ems="10"
        android:hint="@string/AP"
        android:importantForAutofill="no"
        android:text="@string/AP"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.333"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Locations_CIC" />

    <TextView
        android:id="@+id/Locations_TC"
        android:layout_width="408dp"
        android:layout_height="38dp"
        android:ems="10"
        android:hint="@string/TC"
        android:importantForAutofill="no"
        android:text="@string/TC"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Locations_gyms" />

    <TextView
        android:id="@+id/Locations_gyms"
        android:layout_width="408dp"
        android:layout_height="38dp"
        android:ems="10"
        android:hint="@string/Gyms"
        android:importantForAutofill="no"
        android:text="@string/Gyms"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
