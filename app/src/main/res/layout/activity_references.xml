    <TextView
        android:id="@+id/Locations_GL"
        android:layout_width="408dp"
        android:layout_height="38dp"
        android:ems="10"
        android:hint="@string/GL"
        android:importantForAutofill="no"
        android:text="@string/GL"
        android:textSize="18sp"
        android:textStyle="bold"
